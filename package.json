    "@types/react": "^19.2.7",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.15"
  }
}

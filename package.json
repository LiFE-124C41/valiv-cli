{
  "name": "valiv-cli",
  "version": "1.1.0",
  "type": "module",
  "description": "CLI tool for tracking vÎ±-liv activities",
  "main": "dist/cli.js",
  "bin": {
    "valiv": "dist/cli.js"
  },
  "preferGlobal": true,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.tsx --format esm --clean",
    "prepack": "npm run build",
    "dev": "tsup src/cli.tsx --format esm --watch --onSuccess \"node dist/cli.js\"",
    "test": "vitest",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "release": "npm version"
  },
  "keywords": [
    "va-liv",
    "valiv",
    "imas",
    "idolmaster"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LiFE-124C41/valiv-cli.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^14.0.2",
    "conf": "^15.0.2",
    "date-fns": "^4.1.0",
    "ink": "^6.5.1",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "node-ical": "^0.22.1",
    "node-mpv": "^1.5.0",
    "open": "^11.0.0",
    "react": "^19.2.1",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
